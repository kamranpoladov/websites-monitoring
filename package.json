{
  "name": "websites-monitoring",
  "version": "1.0.0",
  "main": "src/main.ts",
  "author": "Kamran Poladov",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -P tsconfig.json -r tsconfig-paths/register src/main.ts shell",
    "start:child": "ts-node -P tsconfig.json -r tsconfig-paths/register src/main.ts monitor",
    "test": "jest",
    "test:types": "tsc --project tsconfig.json",
    "test:lint": "eslint src/ --ext .ts --color",
    "test:prettier": "prettier --check \"./src/**/*.{json,ts}\"",
    "fix:lint": "eslint src/ --ext .ts --color --fix",
    "fix:prettier": "prettier --write \"./src/**/*.{json,ts}\""
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.1.1",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.1.1",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/schedule": "^1.0.1",
    "axios": "^0.23.0",
    "boxen": "^5.1.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "commander": "^8.1.0",
    "moment": "^2.29.1",
    "nestjs-console": "^7.0.0",
    "nestjs-shell": "^1.4.1",
    "normalize-url": "^5.3.0",
    "open-terminal": "git+https://github.com/kamranpoladov/open-terminal.git#production",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "table": "^6.7.2",
    "typescript": "^4.4.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.1.1",
    "@types/cron": "^1.7.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
